<!-- Example test suite -->
<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd" >
<suite name= "book-flight-feature" parallel="tests">
    <test name="book-flight-2-passengers">
        <parameter name ="numOfPassengers" value="2" />
        <parameter name ="expectedPrice" value="$1169 USD" />
        <classes>
            <class name ="com.demoApp.tests.BookFlightTest"/>
        </classes>
    </test>
    <test name="book-flight-3-passengers">
        <parameter name ="numOfPassengers" value="3" />
        <parameter name ="expectedPrice" value="$1753 USD" />
        <classes>
            <class name ="com.demoApp.tests.BookFlightTest"/>
        </classes>
    </test>
</suite>